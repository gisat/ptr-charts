{
  "name": "@gisatcz/ptr-charts",
  "version": "1.3.1",
  "description": "Panther FE charts",
  "keywords": [
    "panther",
    "gisat"
  ],
  "homepage": "https://github.com/gisat/ptr-charts",
  "style": "dist/style.css",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist",
    "lib"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gisat/ptr-charts"
  },
  "dependencies": {
    "@gisatcz/ptr-atoms": "1.2.0",
    "@gisatcz/ptr-utils": "1.2.3",
    "@gisatcz/ptr-core": "1.3.0",
    "@gisatcz/cross-package-react-context": "0.0.1",
    "chroma-js": "2.1.0",
    "classnames": "2.2.6",
    "d3": "5.15.0",
    "d3-sankey": "0.12.3",
    "dom-to-image": "2.6.0",
    "downloadjs": "1.4.7",
    "lodash": "4.17.19",
    "moment": "2.24.0",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-resize-detector": "4.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-alias": "3.0.1",
    "babel-loader": "8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "builtin-modules": "^3.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "3.4.2",
    "glob": "^7.1.6",
    "node-sass": "4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-url": "^8.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.0.4",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-node-externals": "^2.1.3",
    "rollup-plugin-postcss": "^2.5.0",
    "rollup-plugin-sass": "^1.2.2",
    "sass-loader": "8.0.2",
    "source-map-loader": "0.2.4",
    "style-loader": "1.1.3",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11"
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "run-s clean && run-p start:es start:cjs start:lib:es",
    "start:lib:es": "cross-env BABEL_ENV=es babel --watch src -d lib --verbose",
    "start:cjs": "cross-env NODE_ENV=cjs rollup -c -w",
    "start:es": "cross-env NODE_ENV=es rollup -c -w",
    "build": "run-s clean && run-p build:es build:cjs build:lib:es",
    "build:es": "cross-env NODE_ENV=es rollup -c",
    "build:cjs": "cross-env NODE_ENV=cjs rollup -c",
    "build:lib:es": "npm-run-all build:babel:lib:es copy:stylesheets copy:assets",
    "build:babel:lib:es": "cross-env BABEL_ENV=es babel src -d lib",
    "copy:stylesheets": "node ./build/copy-stylesheets.js",
    "copy:assets": "node ./build/copy-assets.js"
  }
}
